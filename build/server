process.addListener('uncaughtException', function (err, stack) {
    console.log('------------------------');
    console.log('Exception: ' + err);
    console.log(err.stack);
    console.log('------------------------');
});

var fs = require("fs"),
    express = require("express"),
    site = express,
    CreateServer = require('./lib/createserver'),

    server = new CreateServer({});










